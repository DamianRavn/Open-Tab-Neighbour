{
  "manifest_version": 3,
  "name": "Open Tab Neighbour",
  "version": "1.0",

  "permissions": ["tabs"],

  "background": {
    "scripts": ["background.js"]
  },

  "commands": 
  {
    "open_tab_next": 
    {
      "suggested_key": 
      {
        "default": "Ctrl+Space"
      },
      "description": "Open a new tab next to the current one"
    },
    "duplicate_tab":
    {
      "suggested_key": 
      {
        "default": "Ctrl+Alt+Space"
      },
      "description": "Duplicates current tab and opens it next to this one"
    }
  }
}



  