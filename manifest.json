{
  "manifest_version": 3,
  "name": "Open Tab Neighbour",
  "version": "1.1",

  "permissions": ["tabs"],

  "background": {
    "scripts": ["background.js"]
  },

  "browser_specific_settings": 
  {
    "gecko": 
    {
      "id": "{365a2b99-cfed-413b-8054-6bb5e4d749f5}"
    }
  },
  

  "commands": 
  {
    "open_tab_next": 
    {
      "suggested_key": 
      {
        "default": "Ctrl+Space"
      },
      "description": "Open a new tab next to the current one"
    },
    "duplicate_tab":
    {
      "suggested_key": 
      {
        "default": "Ctrl+Alt+Space"
      },
      "description": "Duplicates current tab and opens it next to this one"
    },
    "open_yt_sub":
    {
      "suggested_key": 
      {
        "default": "Ctrl+Shift+Space"
      },
      "description": "Open a new Youtube Subscription tab next to the current one"
    }
  }
}



  